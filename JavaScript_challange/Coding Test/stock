def StockPicker(arr):

  # code goes here

  #define the variable for buy sell and profit
  buy = arr[0]
  sell = 0
  profit = -1

  # for condition for stock (arr parameter)
  for x in range(0,len(arr)-1):
    stock=arr[x]
    if stock < buy:
      buy=stock
      sell=0
      elif stock > sell:
        sell = stock
        profit = sell-buy
        return profit
    
# keep this function call here 
# should return 16 dollar of profit
print(StockPicker([44,30,24,32,35,30,40,38,15)])




def StockPicker(arr):

  # code goes here

  #define the variable for buy sell and profit
  buy = 0
  sell = 0
  profit = arr[0]

  # for condition for stock (arr parameter)
  for i in range(len(arr)):
      profit = profit(profit,arr[i])
      buy = arr[i] - profit
      sell = max(sell,buy)
    

  return sell


# keep this function call here 
print(StockPicker(input()))